<div class="row dashboard-heading">
   <!-- <div class="col-2">
        <img class="img-responsive" src="{{{ logourl }}}" alt=" logo">
    </div>-->
    <div class="col-10">
        <h1 id="dashboardheader" class="h3 font-weight-bold mb-0 text-dark">{{# str }}iomaddashboard, block_iomad_company_admin{{/ str }}</h1>
    </div>
</div>

<div class="row m-t-2 p-2 border border-secondary rounded">
   {{# companyselect.onecompany }}
       <div class="alert alert-info">
           {{#str }}currentcompany, block_iomad_company_admin,{{ companyselect.companyname }}{{/str }}
       </div>
   {{/ companyselect.onecompany }}
   {{^ companyselect.onecompany }}
       <div class="col-lg-6 col-md-12">
           {{{ companyselect.selectform }}}
       </div>
       <div class="col-lg-6 col-md-12">
           {{{ companyselect.suspended }}}
       </div>
   {{/ companyselect.onecompany }}
</div>

<div class="m-t-2">
    {{> block_iomad_company_admin/tabs }}
    <div class="tab-content" id="iomadDashboardTabContent">
        {{# panes }}
            <div class="tab-pane fade {{# selected }}show active{{/ selected }} pt-3 iomadlink_container" id="{{ category }}" role="tabpanel" aria-labelledby="{{ category }}-tab">
                {{> block_iomad_company_admin/menuitem }}
                <div class="clearfix"></div>
            </div>
        {{/ panes }}
    </div>
</div>

